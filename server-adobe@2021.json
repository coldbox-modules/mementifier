{
    "name":"mementifier-adobe@2021",
    "app":{
        "serverHomeDirectory":".engine/adobe2021",
        "cfengine":"adobe@2021"
    },
    "web":{
        "http":{
            "port":"60299"
        },
        "rewrites":{
            "enable":"true"
        },
		"webroot":"test-harness",
        "aliases":{
            "/moduleroot/mementifier":"../"
        }
    },
	"JVM":{
		"heapSize":"768",
