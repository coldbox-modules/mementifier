        "args":"-Dcoldfusion.runtime.remotemethod.matchArguments=false"
    },
    "openBrowser":"false",
	"cfconfig":{
        "file":".cfconfig.json"
    },
	"scripts" : {
        "onServerInstall":"cfpm install zip,debugger,orm,mysql,postgresql,sqlserver,feed,chart"
    }
}
